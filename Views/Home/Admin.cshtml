@{
    Layout = null;
}

<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Admin Login Form</title>

    <!-- FAVICON -->
    <link rel="icon" type="image/png" href="~/images/images/favicon.png">

    <!-- Bootstrap 5 cdn links -->
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-1BmE4kWBq78iYhFldvKuhfTAU6auU8tT94WrHftjDbrCEXSU1oBoqyl2QvZ6jIW3" crossorigin="anonymous">
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/5.13.0/css/all.min.css">

    <!-- custom files links -->
    <link rel="stylesheet" href="~/css/style.css">
</head>
<body>
    <div class="wrapper">
        <div class="container">
            <div class="col-sm-8 offset-sm-3 col-lg-6 offset-lg-3 col-xl-4 offset-xl-4 text-center">
                <div class="logo">
                    <img src="~/images/images/logo-new.png" alt="logo" class="img-fluid">
                </div>

                <form method="post" action="/Home/Login" class=" heading-wrapper rounded bg-white shadow p-5">
                    <h3 class="  mb-2" id="heading">Login </h3>
                    <div class="fw-normal text-muted mb-4">
                        Go Back? <a href="/Home/Index" class="text-primary fw-bold text-decoration-none">Home Page</a>
                        <p style="color:red; font-weight:500;">@TempData["Data"]</p>
                    </div>
                    <div class="form-floating mb-3">
                        <input type="text" name="fname" class="form-control" id="floatingName" placeholder="Ali hassan" required>
                        <label for="floatingName">Full Name</label>
                    </div>
                    <div class="form-floating mb-3">
                        <input type="password" name="password" class="form-control" id="floatingEmail" placeholder="1234">
                        <label for="floatingEmail">Enter Passwoed</label>
                    

                    <input type="submit" value="Submit" class="btn  submit-btn w-100 my-4">

                </form>

            </div>
        </div>
    </div>
    <!-- Bootstrap 5 Js cdn links -->
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.0.2/dist/js/bootstrap.bundle.min.js" integrity="sha384-MrcW6ZMFYlzcLA8Nl+NtUVF0sA7MsXsP1UyJoMp4YLEuNSfAP+JcXn/tWtIaxVXM" crossorigin="anonymous"></script>
    <!-- Custom js links -->
    <script src="~/js/main.js"></script>

</body>
</html>
